<uexplugins>
    <plugin
        className="org.zywx.wbpalmstar.plugin.uexxunfei.EUExXunfei"
        uexName="uexXunfei">
        <method
            name="init"
            type="1"/>
        <method
            name="initSpeaker"
            type="1"/>
        <method name="startSpeaking"/>
        <method name="stopSpeaking"/>
        <method name="pauseSpeaking"/>
        <method name="resumeSpeaking"/>
        <method
            name="onSpeakComplete"
            type="2"/>
        <method
            name="onSpeakBegin"
            type="2"/>
        <method
            name="onSpeakPaused"
            type="2"/>
        <method
            name="onSpeakResumed"
            type="2"/>

        <method
            name="initRecognizer"
            type="1"/>
        <method name="startListening"/>
        <method name="stopListening"/>
        <method name="cancelListening"/>
        <method
            name="onRecognizeError"
            type="2"/>
        <method
            name="onRecognizeResult"
            type="2"/>
        <method
            name="onVolumeChanged"
            type="2"/>
        <method
            name="onBeginOfSpeech"
            type="2"/>
        <method
            name="onEndOfSpeech"
            type="2"/>
    </plugin>
</uexplugins>